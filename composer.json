{
    "name": "bobdenotter/chmodinator",
    "description": "An extension to change file-rights (permissions) on files, so both you and Bolt can modify them.",
    "type": "bolt-extension",
    "keywords": ["chmod", "chmodinator", "rights", "filerights", "777", "rwx", "chown", "permissions"],
    "require": {
        "bolt/bolt": "^3.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bob den Otter",
            "email": "bob@twokings.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Bobdenotter\\Chmodinator\\": "src"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Bobdenotter\\Chmodinator\\ChmodinatorExtension"
    }
}
